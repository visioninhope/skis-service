    "tui-editor-plugin-font-size": "1.0.4",
    "vee-validate": "4.12.4",
    "video.js": "8.6.1",
    "vis-data": "7.1.9",
    "vis-network": "9.1.9",
    "vue": "3.3.4",
    "vue-apexcharts": "1.6.2",
    "vue-radial-progress": "0.3.2",
    "vue-router": "4.2.5",
    "vue-scrollto": "2.20.0",
    "vue-select": "3.20.2",
    "vue-simple-progress": "1.1.1",
    "vue-simple-spinner": "1.2.10",
